{
  "name": "Juli",
  "description": "AppView template for WebCatalog",
  "version": "9.0.0-alpha",
  "minimumWebCatalogVersion": "23.0.0",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "npx patch-package && npx electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src --ext js",
    "lint:fix": "eslint ./public ./src --ext js --fix",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\" -k",
    "build-json": "node build-json.js",
    "build-template": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false node build-template.js",
    "build-zip": "7z a -mm=Deflate -mx=9 -r dist/template.zip dist/template/.",
    "dist": "yarn build && yarn build-template && yarn build-zip && yarn build-json"
  },
  "repository": "https://github.com/atomery/webcatalog",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "dependencies": {
    "@cliqz/adblocker-electron": "1.17.0",
    "cheerio": "1.0.0-rc.3",
    "darkreader": "4.9.15",
    "electron-context-menu": "2.2.0",
    "electron-is-dev": "1.2.0",
    "electron-settings": "3.2.0",
    "electron-window-state": "5.0.3",
    "follow-redirects": "1.12.1",
    "fs-extra": "9.0.1",
    "is-url": "1.2.4",
    "jimp": "0.14.0",
    "menubar": "8.0.2",
    "node-fetch": "2.6.0",
    "proxy-agent": "3.1.1",
    "regedit": "3.0.3",
    "semver": "7.3.2",
    "tmp": "0.2.1",
    "uuid": "8.2.0"
  },
  "devDependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "ace-builds": "1.4.12",
    "algoliasearch": "4.3.1",
    "babel-eslint": "10.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "date-fns": "2.15.0",
    "electron": "8.4.1",
    "electron-builder": "22.8.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.4",
    "eslint-plugin-react-hooks": "2.5.1",
    "hasha": "5.2.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-ace": "9.1.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.1",
    "react-scripts": "3.4.1",
    "react-sortable-hoc": "1.11.0",
    "react-window": "1.8.5",
    "react-window-infinite-loader": "1.0.5",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "simplebar": "5.2.1",
    "simplebar-react": "2.2.1",
    "typeface-roboto": "0.0.75",
    "wait-on": "5.1.0"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
